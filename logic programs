public class test4 {

	public static void main(String args[])
	{
		String s1=new String("Tech");
		String s2=new String("Tech");    // only equals true  (purpose: compare objects )

			//or
				
		//String s1="Tech";              // both are true  == , equals (purpose: primitive data type )
		//String s2="Tech";
		
		
		if(s1==s2)
		{
			System.out.println("==");
		}
	   if(s1.equals(s2))
		{
			System.out.println("equals");
		}
		
		
	}
}
